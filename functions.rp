fn max(i64 a, i64 b): i64 {
  if (a > b) {
    return a;
  }
  return b;
}

fn fib(i64 x): i64 {
  if (x == 1) {
    return 1;
  }

  if (x == 2) {
    return 1;
  }

  i64 a = fib(x - 1);
  i64 b = fib(x - 2);
  return a + b;
  #return fib(x - 1) + fib(x - 2);
}

fn sum(i64 x): i64 {
  if (x == 1) {
    return 1;
  }

  return x + sum(x - 1);
}

i64 x = sum(4);
